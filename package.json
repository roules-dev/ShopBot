{
  "name": "shopbot",
  "version": "2.2.1",
  "author": "@roules_",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npx tsc-alias",
    "clean:dist": "(rd /s /q dist || rm -rf dist) && mkdir dist",
    "cleanbuild": "npm run clean:dist --silent && npm run build",
    "prodbuild": "npm run clean:dist --silent && tsc --build --sourceMap false && npx tsc-alias",
    "dev": "nodemon src/index.ts",
    "start": "npm run build && npm run serve",
    "serve": "node dist/index.js",
    "deploy": "node ./dist/deploy-commands.js",
    "test": "npm run build && node ./dist/test.js",
    "setup": "npm i && node ./dist/tools/setup-helper.js",
    "nanoid": "node ./dist/tools/migrate-db-to-nanoid.js"
  },
  "license": "GPL-3.0",
  "devDependencies": {
    "@discordjs/rest": "^2.2.0",
    "@types/luxon": "^3.4.2",
    "@types/node": "^25.0.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "nodemon": "^3.1.10",
    "prettier": "3.8.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "typescript-eslint": "^8.54.0"
  },
  "dependencies": {
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "fs": "^0.0.1-security",
    "luxon": "^3.5.0",
    "nanoid": "^5.1.6"
  }
}
